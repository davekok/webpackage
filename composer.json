{
    "name": "davekok/webpackage",
    "description": "Webpackage tool",
    "type": "library",
    "license": "UNLICENSE",
    "authors": [
        {
            "name": "Dave Kok",
            "email": "email@davekok.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "davekok\\webpackage\\": "src/",
            "davekok\\webpackage\\tests\\": "tests/"
        }
    },
    "bin": [
        "bin/wpk"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/davekok/system"
        },
        {
            "type": "vcs",
            "url": "https://github.com/davekok/kernel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/davekok/parser"
        }
    ],
    "require": {
        "davekok/system": "dev-main",
        "davekok/kernel": "dev-main",
        "davekok/parser": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2.x-dev"
    }
}
